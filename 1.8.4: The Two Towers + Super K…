function main() {
    buildTower();
    buildTower2();
    // Write your code here
}

function buildTower() {
    move();
    turnLeft();
    putBall();
    move();
    putBall();
    move();
    putBall();
    turnLeft();
    turnLeft();
    move();
    move();
    turnLeft();
}

function buildTower2() {
    move();
    move();
    turnLeft();
    putBall();
    move();
    putBall();
    move();
    putBall();
    move();
    turnRight();
}


// Call main at the end so it runs
main();
